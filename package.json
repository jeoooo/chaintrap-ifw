    "storybook": "^7.1.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vite-plugin-node-polyfills": "^0.9.0",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.4.5",
